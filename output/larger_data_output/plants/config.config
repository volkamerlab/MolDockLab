# search algorithm
search_speed speed1
aco_ants 20
flip_amide_bonds 0
flip_planar_n 1
force_flipped_bonds_planarity 0
force_planar_bond_rotation 1
rescore_mode simplex
flip_ring_corners 0
# scoring functions
# Intermolecular (protein-ligand interaction scoring)
scoring_function chemplp
outside_binding_site_penalty 50.0
enable_sulphur_acceptors 1
# Intramolecular ligand scoring
ligand_intra_score clash2
chemplp_clash_include_14 1
chemplp_clash_include_HH 0
# input
protein_file /home/hamza/github/MolDockLab/ECFT-VS-pipeline/test_data/5UG9_noligand_protoss.mol2
ligand_file /home/hamza/github/MolDockLab/ECFT-VS-pipeline/output/larger_data_output/test_library_10cpds_prepared.mol2
# output
output_dir /home/hamza/github/MolDockLab/ECFT-VS-pipeline/output/larger_data_output/plants/temp
# write single mol2 files (e.g. for RMSD calculation)
write_multi_mol2 1
# binding site definition
bindingsite_center -13.8704 14.9338 -26.7299
bindingsite_radius 7.02146
# cluster algorithm
cluster_structures 10
cluster_rmsd 2.0
# write
write_ranking_links 0
write_protein_bindingsite 0
write_protein_conformations 0
write_protein_splitted 0
write_merged_protein 0
